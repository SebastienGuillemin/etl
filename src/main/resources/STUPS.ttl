#################################################################
#    Classes
#################################################################

###  http://www.stups.fr/ontologies/2023/stups/#Aspect
:Aspect rdf:type owl:Class ;
        owl:equivalentClass [ rdf:type owl:Restriction ;
                              owl:onProperty :estAspectDe ;
                              owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                              owl:onClass :Echantillon
                            ] ;
        rdfs:comment "Un aspect décrit l’aspect interne ou externe d’un échantillon." .


###  http://www.stups.fr/ontologies/2023/stups/#Composant
:Composant rdf:type owl:Class ;
           owl:equivalentClass [ owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                        owl:onProperty :aSubstance ;
                                                        owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                        owl:onClass :Substance
                                                      ]
                                                      [ rdf:type owl:Restriction ;
                                                        owl:onProperty :dosage ;
                                                        owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                        owl:onDataRange xsd:float
                                                      ]
                                                    ) ;
                                 rdf:type owl:Class
                               ] ;
           rdfs:comment "Un composant d'un échantillon est soit un principe actif soit un produit de coupage. Un composant est lié à une substance et éventuellement à un dosage (exprimé en pourcentage massique)." .


###  http://www.stups.fr/ontologies/2023/stups/#Departement
:Departement rdf:type owl:Class ;
             owl:equivalentClass [ owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                          owl:onProperty :aVille ;
                                                          owl:someValuesFrom :Ville
                                                        ]
                                                        [ rdf:type owl:Restriction ;
                                                          owl:onProperty :estDansRegion ;
                                                          owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                          owl:onClass :Region
                                                        ]
                                                      ) ;
                                   rdf:type owl:Class
                                 ] ;
             rdfs:comment "Un département est dans une région et a une ou plusieurs villes." .


###  http://www.stups.fr/ontologies/2023/stups/#Echantillon
:Echantillon rdf:type owl:Class ;
             owl:equivalentClass [ owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                          owl:onProperty :aProduitCoupage ;
                                                          owl:someValuesFrom :ProduitCoupage
                                                        ]
                                                        [ rdf:type owl:Restriction ;
                                                          owl:onProperty :estLieA ;
                                                          owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger ;
                                                          owl:onClass :Echantillon
                                                        ]
                                                        [ rdf:type owl:Restriction ;
                                                          owl:onProperty :aAspectExterne ;
                                                          owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                          owl:onClass :Aspect
                                                        ]
                                                        [ rdf:type owl:Restriction ;
                                                          owl:onProperty :aPrincipeActif ;
                                                          owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                          owl:onClass :PrincipeActif
                                                        ]
                                                        [ rdf:type owl:Restriction ;
                                                          owl:onProperty :provientDe ;
                                                          owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                          owl:onClass :Scelle
                                                        ]
                                                        [ rdf:type owl:Restriction ;
                                                          owl:onProperty :aAspectInterne ;
                                                          owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                          owl:onClass :Aspect
                                                        ]
                                                        [ rdf:type owl:Restriction ;
                                                          owl:onProperty :aProfilChimique ;
                                                          owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                          owl:onClass :ProfilChimique
                                                        ]
                                                        [ rdf:type owl:Restriction ;
                                                          owl:onProperty :caracteristiqueMacroscopique ;
                                                          owl:someValuesFrom rdfs:Literal
                                                        ]
                                                        [ rdf:type owl:Restriction ;
                                                          owl:onProperty :id ;
                                                          owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                          owl:onDataRange xsd:integer
                                                        ]
                                                        [ rdf:type owl:Restriction ;
                                                          owl:onProperty :numeroEchantillon ;
                                                          owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                          owl:onDataRange xsd:string
                                                        ]
                                                        [ rdf:type owl:Restriction ;
                                                          owl:onProperty :typeDrogue ;
                                                          owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                          owl:onDataRange xsd:string
                                                        ]
                                                      ) ;
                                   rdf:type owl:Class
                                 ] ;
             owl:hasKey ( :id
                        ) ;
             rdfs:comment "Un échantillon de stupéfiant appartient à un scellé. Un échantillon est caractérisé par un numéro d’échantillon (une chaîne de caractères), son type de drogue ('cannabis', 'cocaïne', 'divers' ou 'amphétamine et dérivés'), des caractéristiques macroscopiques, un aspect externe, potentiellement un aspect interne, un principe actif et des produits de coupage. Les experts peuvent aussi donner des commentaires sur l’échantillon. De plus, un échantillon peut être lié à d'autres échantillons. Dans le cas des stupéfiants sur lesquels une analyse de profilage chimique est réalisée, chaque échantillon est associé à un profil chimique." .


###  http://www.stups.fr/ontologies/2023/stups/#FormeChimique
:FormeChimique rdf:type owl:Class ;
               owl:equivalentClass [ rdf:type owl:Class ;
                                     owl:oneOf ( :Base
                                                 :BaseEtHCL
                                                 :Chlorhydrate
                                                 :HCL
                                                 :Indeterminee
                                                 :Phosphate
                                                 :Sulfate
                                               )
                                   ] ;
               rdfs:comment "Une forme chimique est associée à un principe actif pour décrire sa forme chimique. Les formes chimiques considérées dans l'ontologie STUPS sont : Base, HCL, Base et HCL, Sulfate, Chlorhydrate, Phosphate ou Indéterminée." .


###  http://www.stups.fr/ontologies/2023/stups/#Pays
:Pays rdf:type owl:Class ;
      owl:equivalentClass [ rdf:type owl:Restriction ;
                            owl:onProperty :aRegion ;
                            owl:someValuesFrom :Region
                          ] ;
      rdfs:comment "Un pays a une ou plusieurs régions." .


###  http://www.stups.fr/ontologies/2023/stups/#Pic
:Pic rdf:type owl:Class ;
     owl:equivalentClass [ owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                  owl:onProperty :represente ;
                                                  owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                  owl:onClass :Substance
                                                ]
                                                [ rdf:type owl:Restriction ;
                                                  owl:onProperty :estPicDe ;
                                                  owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                  owl:onClass :ProfilChimique
                                                ]
                                                [ rdf:type owl:Restriction ;
                                                  owl:onProperty :airPic ;
                                                  owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                  owl:onDataRange xsd:float
                                                ]
                                                [ rdf:type owl:Restriction ;
                                                  owl:onProperty :tempsRetention ;
                                                  owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                  owl:onDataRange xsd:float
                                                ]
                                              ) ;
                           rdf:type owl:Class
                         ] ;
     rdfs:comment "Un pic d’un profil chimique caractérise un composé chimique (molécule) présent dans l’échantillon. Chaque pic est caractérisé par un temps de rétention et une aire, correspondant à la quantité du composé cible dans l’échantillon." .


###  http://www.stups.fr/ontologies/2023/stups/#PrincipeActif
:PrincipeActif rdf:type owl:Class ;
               owl:equivalentClass [ owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                            owl:onProperty :aFormeChimique ;
                                                            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                            owl:onClass :FormeChimique
                                                          ]
                                                          [ rdf:type owl:Restriction ;
                                                            owl:onProperty :estPrincipeActifDe ;
                                                            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                            owl:onClass :Echantillon
                                                          ]
                                                        ) ;
                                     rdf:type owl:Class
                                   ] ;
               rdfs:subClassOf :Composant ;
               owl:disjointWith :ProduitCoupage ;
               rdfs:comment "Un principe actif est la principale substance d’un échantillon. Tout principe actif a une forme chimique." .


###  http://www.stups.fr/ontologies/2023/stups/#ProduitCoupage
:ProduitCoupage rdf:type owl:Class ;
                owl:equivalentClass [ rdf:type owl:Restriction ;
                                      owl:onProperty :estProduitCoupageDe ;
                                      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                      owl:onClass :Echantillon
                                    ] ;
                rdfs:subClassOf :Composant ;
                rdfs:comment "Un produit de coupage est une substance composant un échantillon." .


###  http://www.stups.fr/ontologies/2023/stups/#ProfilChimique
:ProfilChimique rdf:type owl:Class ;
                owl:equivalentClass [ owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                             owl:onProperty :aPic ;
                                                             owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                             owl:onClass :Pic
                                                           ]
                                                           [ rdf:type owl:Restriction ;
                                                             owl:onProperty :estProfilChimiqueDe ;
                                                             owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                             owl:onClass :Echantillon
                                                           ]
                                                         ) ;
                                      rdf:type owl:Class
                                    ] ;
                rdfs:comment "Un profil chimique est associé à un échantillon dans le but de faire du profilage chimique. Un profil chimique représente un chromatogramme constitué d’un ensemble de pics permettant d’identifier la présence de composés chimiques cibles." .


###  http://www.stups.fr/ontologies/2023/stups/#ProfilChimiqueCocaine
:ProfilChimiqueCocaine rdf:type owl:Class ;
                       owl:equivalentClass [ owl:intersectionOf ( :ProfilChimique
                                                                  [ rdf:type owl:Restriction ;
                                                                    owl:onProperty :estProfilChimiqueDe ;
                                                                    owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                                    owl:onClass :Echantillon
                                                                  ]
                                                                  [ rdf:type owl:Restriction ;
                                                                    owl:onProperty :aPic ;
                                                                    owl:maxQualifiedCardinality "18"^^xsd:nonNegativeInteger ;
                                                                    owl:onClass :Pic
                                                                  ]
                                                                ) ;
                                             rdf:type owl:Class
                                           ] ;
                       owl:disjointWith :ProfilChimiqueHeroine ;
                       rdfs:comment "Un profil de cocaïne a 18 pics correspondant à des solvants résiduels ayant servi dans la fabrication de la cocaïne. Ils ne sont pas toujours tous présents, cela dépend du procédé de fabrication." .


###  http://www.stups.fr/ontologies/2023/stups/#ProfilChimiqueHeroine
:ProfilChimiqueHeroine rdf:type owl:Class ;
                       owl:equivalentClass [ owl:intersectionOf ( :ProfilChimique
                                                                  [ rdf:type owl:Restriction ;
                                                                    owl:onProperty :estProfilChimiqueDe ;
                                                                    owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                                    owl:onClass :Echantillon
                                                                  ]
                                                                  [ rdf:type owl:Restriction ;
                                                                    owl:onProperty :aPic ;
                                                                    owl:maxQualifiedCardinality "6"^^xsd:nonNegativeInteger ;
                                                                    owl:onClass :Pic
                                                                  ]
                                                                ) ;
                                             rdf:type owl:Class
                                           ] ;
                       rdfs:comment "Un profil d'héroïne a 6 pics correspondant à des impuretés dépendant du procédé de fabrication (synthèse). Elles sont toujours présentes mais les quantités peuvent être différentes." .


###  http://www.stups.fr/ontologies/2023/stups/#Region
:Region rdf:type owl:Class ;
        owl:equivalentClass [ owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                     owl:onProperty :aDepartement ;
                                                     owl:someValuesFrom :Departement
                                                   ]
                                                   [ rdf:type owl:Restriction ;
                                                     owl:onProperty :estDansPays ;
                                                     owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                     owl:onClass :Pays
                                                   ]
                                                 ) ;
                              rdf:type owl:Class
                            ] ;
        rdfs:comment "Une région a un ou plusieurs départements et est dans un pays." .


###  http://www.stups.fr/ontologies/2023/stups/#Saisine
:Saisine rdf:type owl:Class ;
         owl:equivalentClass [ owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                      owl:onProperty :aScelle ;
                                                      owl:someValuesFrom :Scelle
                                                    ]
                                                    [ rdf:type owl:Restriction ;
                                                      owl:onProperty :aVilleSaisine ;
                                                      owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                      owl:onClass :Ville
                                                    ]
                                                    [ rdf:type owl:Restriction ;
                                                      owl:onProperty :aPaysDestination ;
                                                      owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                      owl:onClass :Pays
                                                    ]
                                                    [ rdf:type owl:Restriction ;
                                                      owl:onProperty :aServiceCapteur ;
                                                      owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                      owl:onClass :ServiceCapteur
                                                    ]
                                                    [ rdf:type owl:Restriction ;
                                                      owl:onProperty :aServiceRequerant ;
                                                      owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                      owl:onClass :ServiceRequerant
                                                    ]
                                                    [ rdf:type owl:Restriction ;
                                                      owl:onProperty :date ;
                                                      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                      owl:onDataRange xsd:dateTime
                                                    ]
                                                  ) ;
                               rdf:type owl:Class
                             ] ;
         owl:hasKey ( :id
                    ) ;
         rdfs:comment "Une saisine est un terme juridique utilisé en matière de procédure. Elle désigne l'action qu'accomplit un requérant lorsqu'il demande l'analyse d'une saisie de stupéfiant. La fiche (i.e. l’ensemble des informations) d’une saisine d’un produit supposé illicite contient un service capteur, un service requérant, la ville où la saisine a été faite et le pays de destination du produit. Ces renseignements peuvent être complets ou partiels. De plus, une saisie contient un ensemble de scellés." .


###  http://www.stups.fr/ontologies/2023/stups/#Scelle
:Scelle rdf:type owl:Class ;
        owl:equivalentClass [ owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                     owl:onProperty :aEchantillon ;
                                                     owl:someValuesFrom :Echantillon
                                                   ]
                                                   [ rdf:type owl:Restriction ;
                                                     owl:onProperty :estDansSaisine ;
                                                     owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                     owl:onClass :Saisine
                                                   ]
                                                   [ rdf:type owl:Restriction ;
                                                     owl:onProperty :id ;
                                                     owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                     owl:onDataRange xsd:integer
                                                   ]
                                                 ) ;
                              rdf:type owl:Class
                            ] ;
        owl:hasKey ( :id
                   ) ;
        rdfs:comment "Les scellés sont contenus dans une saisine. Chaque scellé contient un ou plusieurs échantillons des produits à identifier et est identifié par un numéro unique. Un scellé est une mesure ordonnée, afin de conserver les pièces à conviction à la disposition de la justice. Il peut désigner également le fait d'apposer un cachet de cire sur une pièce à conviction." .


###  http://www.stups.fr/ontologies/2023/stups/#Service
:Service rdf:type owl:Class ;
         owl:equivalentClass [ owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                      owl:onProperty :estSitueA ;
                                                      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                      owl:onClass :Ville
                                                    ]
                                                    [ rdf:type owl:Restriction ;
                                                      owl:onProperty :nomService ;
                                                      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                      owl:onDataRange xsd:string
                                                    ]
                                                  ) ;
                               rdf:type owl:Class
                             ] ;
